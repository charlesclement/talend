package org.talend.survivorship.sample 

rule "OSM_country_code"
    no-loop true
    dialect "mvel"
    ruleflow-group "OSM_country_codeGroup"
when
    $input : RecordIn( dataset.isMostComplete( $input.TALEND_INTERNAL_ID ) )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "OSM_country_code", drools.getRule().getName());

end


