package Survivor 

rule "VAT_requestDate"
    no-loop true
    dialect "mvel"
    ruleflow-group "VAT_requestDateGroup"
when
    $input : RecordIn( dataset.isMostComplete( $input.TALEND_INTERNAL_ID ) )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "VAT_requestDate", drools.getRule().getName());

end


