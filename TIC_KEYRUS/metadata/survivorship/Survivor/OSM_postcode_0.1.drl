package Survivor 

rule "OSM_postcode"
    no-loop true
    dialect "mvel"
    ruleflow-group "OSM_postcodeGroup"
when
    $input : RecordIn( dataset.isMostComplete( $input.TALEND_INTERNAL_ID ) )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "OSM_postcode", drools.getRule().getName());

end


