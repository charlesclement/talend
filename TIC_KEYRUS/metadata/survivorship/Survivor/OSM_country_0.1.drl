package Survivor 

rule "OSM_country"
    no-loop true
    dialect "mvel"
    ruleflow-group "OSM_countryGroup"
when
    $input : RecordIn( dataset.isMostComplete( $input.TALEND_INTERNAL_ID ) )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "OSM_country", drools.getRule().getName());

end


