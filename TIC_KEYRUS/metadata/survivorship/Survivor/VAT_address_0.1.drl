package Survivor 

rule "VAT_address"
    no-loop true
    dialect "mvel"
    ruleflow-group "VAT_addressGroup"
when
    $input : RecordIn( dataset.isMostComplete( $input.TALEND_INTERNAL_ID ) )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "VAT_address", drools.getRule().getName());

end


